   <children>
      <BarChart fx:id="barChart" title="Comparativa de precios - Series Q">
        <xAxis>
          <CategoryAxis label="Modelos" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis autoRanging="false" label="Precios" side="LEFT" tickUnit="25000.0" upperBound="200000.0" />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
